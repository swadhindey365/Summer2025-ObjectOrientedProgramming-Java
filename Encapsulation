class Account {
    private double balance = 1000.00;

    public double getBalance() {
        return balance;
    }

    public void deposit(double amount) {
        if (amount > 0)
            balance += amount;
    }
}
public class GitHub {
    public static void main(String[] args) {
        Account acc = new Account();
        acc.deposit(500);
        System.out.println("Balance: " + acc.getBalance());
    }
}
